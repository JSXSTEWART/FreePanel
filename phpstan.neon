includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app
        - config
        - database
        - routes
    level: 5
    ignoreErrors:
    excludePaths:
        - bootstrap/cache/*
        - storage/*
        - vendor/*
    tmpDir: storage/phpstan
    checkMissingIterableValueType: false
